{
  "name": "write-doc",
  "version": "1.0.0-alpha.3",
  "private": false,
  "description": "An environment to document",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.62",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "copyfiles": "^2.4.1",
    "css-minify": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "renamer": "^4.0.0",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-router-dom": "^6.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "rename": "renamer --find .min.css --replace .css lib/css/*",
    "css": "css-minify -d src/lib/css -o lib/css && npm run rename",
    "fonts": "copyfiles --flat src/lib/fonts/** lib/fonts",
    "lib": "tsc -p ./tsconfig.build.json && npm run css && npm run fonts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "author": "Erick <ertrii@live.com> (https://github.com/ertrii)",
  "license": "MIT",
  "bugs": "https://github.com/ertrii/write-doc/issues",
  "repository": "https://github.com/ertrii/write-doc",
  "keywords": [
    "react",
    "documentation"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "lib",
    "Readme.md",
    "LICENSE"
  ]
}
